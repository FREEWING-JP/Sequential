/* Screen names in the Use Screen menu. %d is replaced with the screen number. */
"Main Screen" = "Główny ekran";
"Screen %d" = "Ekran %d";

/* Clear the Open Recent menu. Should be the same as the standard text. */
"Clear Menu" = "Menu Wymaż";

/* Label for when no image is being displayed in the window. */
"No image" = "No image";

/* Enter/exit full screen. Two states of the same item. */
"Enter Full Screen" = "Enter Full Screen";
"Exit Full Screen" = "Exit Full Screen";

/* Lets the user toggle the on-screen display. Two states of the same item. */
"Show Info" = "Pokaż informacje";
"Hide Info" = "Ukryj informacje";

/* Toggle the Exif panel. Two states. */
"Show Exif Data" = "Show Exif Data";
"Hide Exif Data" = "Hide Exif Data";

/* Scale image down so the entire thing fits menu item. Two labels, depending on mode. */
"Fit to Screen" = "Dopasuj do ekranu";
"Fit to Window" = "Fit to Window";

/* The title of the bookmarks menu. Two states. */
"Resume" = "Przywróć";
"Delete..." = "Delete…";

/* Bookmark menu item used when the file named %@ cannot be found. */
"Missing File %@" = "Brak pliku “%@”";

/* Folder with no displayable images error. %@ is replaced by the filename. */
"The folder %@ contains no displayable images." = "Katalog “%@” zawiera niemożliwe do wyświetlenia obrazy.";
"If one of this folder's subfolders contains images you wish to view, open that folder directly." = "Jeśli któryś z podkatalogów zawiera obrazy, które chcesz obejrzeć, musisz otworzyć ten podkatalog bezpośrednio";

/* Archive with no displayable images error. %@ is replaced by the filename. */
"The archive %@ contains no displayable images." = "Archiwum “%@” zawiera niemożliwe do wyświetlenia obrazy.";
"If you know it does, try expanding it with another application." = "Jeśli wiesz, że jest inaczej, spróbuj otworzyć plik(i) innym programem.";

/* Invalid archive type error. %@ is replaced by the filename. */
"The file %@ does not appear to be in a supported format." = "Plik “%@” nie wydaje się być wspieranym formatem plików.";
"If this file is an archive, try expanding it with another application." = "Jeśli ten plik jest archiwum, to spróbuj otworzyć je innym programem.";

/* Archive requires a password prompt. %@ is replaced by the filename. */
"The archive %@ is encrypted and requires a password in order to be opened." = "Archiwum “%@” jest zaszyfrowane i wymaga hasła, aby je otworzyć.";

/* The archive has an unknown encoding prompt. %@ is replaced by the filename. */
"The correct text encoding for the file %@ could not be automatically determined." = "Prawidłowe kodowanie tekstu dla pliku “%@” nie zostało automatycznie określone.";

/* The URL returned a error status code. %u is replaced by the status code, the first %@ is replaced by the human-readable error (automatically localized), the second %@ is replaced by the full URL. */
"The error %u %@ was generated while loading the URL %@." = "Błąd “%u %@” powstał podczas próby załadowania adresu URL “%@.”";
"Make sure the URL is correct and the page is available." = "Upewnij się, że URL jest prawidłowy i strona jest dostępna.";

/* The URL could not be loaded for an unknown reason. %@ is replaced by the full URL. */
"The URL %@ could not be loaded." = "Nie udało się załadować adresu URL “%@” .";
"Make sure the connection and server are available." = "Upewnij się, że połączenie i serwer są dostępne.";

/* The URL didn't have any images, and was not an image itself. %@ is replaced by the full URL. */
"The URL %@ did not contain any displayable images." = "Adres URL “%@” nie zawiera żadnych możliwych do wyświetlenia obrazów.";
"Sequential displays images that are linked to by or embedded within the page." = "Sequential wyświetla obrazy, których odnośniki znajdują się na stronie.";

/* The URL is being loaded. %@ is replaced by the full URL. */
"Sequential is loading the URL %@." = "Sequential ładuje adres URL “%@.”";
"Calculating time remaining..." = "Obliczam pozostały czas...";

/* The URL is being loaded. %@ is replaced by a number with the units below. */
"Loading URL: About %@ remaining." = "Ładowanie adresu URL: Pozostało około %@.";
"Scanning Links: About %@ remaining." = "Skanowanie odnośników: Pozostało około  %@";

/* Units used for time remaining. Only the largest unit is shown. %u is replaced by a non-1 number. */
"%u hours" = "%u godzin";
"%u minutes" = "%u minut";
"%u seconds" = "%u sekund";

/* Units used for time remaining when the amount is 1. */
"1 hour" = "1 godzina";
"1 minute" = "1 minuta";
"1 second" = "1 sekunda";

/* Replace file alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"%@ already exists in %@. Do you want to replace it?" = "“%@” już istnieje w “%@.” Czy chcesz to zastąpić?";

/* Replace multiple files alert. %u is replaced with a number greater than 1, %@ is replaced with the destination name. */
"%u pages already exist in %@. Do you want to replace them?" = "Strony %u już istnieją w “%@.” Czy chcesz je zastąpić?";

/* Informative text for replacement alerts. */
"Replacing a file overwrites its current contents." = "Zastąpienie pliku nadpisuje jego zawartość.";

/* Extraction window title. */
"Extract" = "Zachowaj";

/* Extraction single failure alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"The page %@ could not be extracted to %@." = "Nie można było zachować strony “%@” w “%@.”";

/* Extraction multiple failure alert. %u is replaced with the number of files, %@ is replaced with the destination name. */
"%u pages could not be extracted to %@." = "Nie można było zachować stron %u w “%@.”";

/* Informative text for extraction failure alerts. */
"Make sure the volume is writable and has enough free space." = "Upewnij się, że ten wolumin można zapisywać, oraz że posiada wystarczająco wolnego miejsca.";

/* Appears in the extraction alert beside each filename that conflicts with an existing file in the destination folder. */
"File already exists." = "Plik już istnieje.";

/* Confirmation dialog when the user intentionally deletes a bookmark. %@ is the bookmarked file's name. */
"Are you sure you want to delete the bookmark %@?" = "Are you sure you want to delete the bookmark “%@?”";

/* Confirmation dialog informative text. */
"This operation cannot be undone." = "This operation cannot be undone.";

/* Bookmarked file could not be found error. %@ is replaced with the missing page's saved filename. */
"The file referenced by the bookmark %@ could not be found." = "Plik wskazywany przez zakładkę “%@” nie został znaleziony.";

/* Bookmarked file could not be found error informative text. */
"It may have been moved or deleted." = "Mógł zostać przeniesiony lub usunięty.";

/* Too many open files alert message. %@ is replaced with the document name. */
"The document %@ could not be opened because Sequential reached the system's per-application limit on number of open files." = "The document “%@” could not be opened because Sequential reached the system's per-application limit on number of open files.";

/* Too many open files alert informative text. %@ is replaced with another sentence. */
"Try opening folders with fewer files. %@" = "Try opening folders with fewer files. %@";

/* One of the additional sentences for the too many open files alert. */
"The current limit could not be determined." = "The current limit could not be determined.";

/* One of the additional sentences for the too many open files alert. %d is replaced with the limit number. */
"The current limit is %d, but may be slightly less in practice." = "The current limit is %d, but may be slightly less in practice.";

/* Dialog button labels. */
"OK" = "OK";
"Cancel" = "Anuluj";
"Delete Bookmark" = "Usuń zakładkę";
"Replace" = "Zastąp";
"Choose" = "Wybierz";