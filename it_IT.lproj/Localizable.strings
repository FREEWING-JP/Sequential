/* The primary screen. */
"Main Screen" = "Schermo principale";

/* Non-primary screens. %u is replaced with the screen number. */
"Screen %u" = "Schermo %u";

/* Clear the Open Recent menu. Should be the same as the standard text. */
"Clear Menu" = "Cancella Menu";

/* Label for when no image is being displayed in the window. */
"No image" = "Nessuna immagine";

/* Reveal in Finder, Path Finder (www.cocoatech.com) or web browser. Three states of the same item. */
"Reveal in Finder" = "Rivela nel Finder";
"Reveal in Path Finder" = "Rivela in Path Finder";
"Reveal in Browser" = "Mostra nel browser";

/* Switch to Finder or Path Finder (www.cocoatech.com). Two states of the same item. */
"Switch to Finder" = "Passa al Finder";
"Switch to Path Finder" = "Passa a Path Finder";

/* Enter/exit full screen. Two states of the same item. */
"Enter Full Screen" = "Vai a schermo pieno";
"Exit Full Screen" = "Esci dallo schermo pieno";

/* Lets the user toggle the on-screen display. Two states of the same item. */
"Show Info" = "Mostra informazioni";
"Hide Info" = "Nascondi informazioni";

/* Lets the user toggle whether thumbnails are shown. Two states of the same item. */
"Show Thumbnails" = "Show Thumbnails";
"Hide Thumbnails" = "Hide Thumbnails";

/* Scale image down so the entire thing fits menu item. Two labels, depending on mode. */
"Fit to Screen" = "Adatta allo schermo";
"Fit to Window" = "Adatta alla finestra";

/* The title of the bookmarks menu. Two states. */
"Resume" = "Riprendi";
"Delete..." = "Cancella…";

/* Bookmark menu item used when the file named %@ cannot be found. */
"Missing File %@" = "File “%@” mancante";

/* Display string for timer intervals. %@ is replaced with the remaining seconds and tenths of seconds.*/
"%@ seconds" = "%@ secondi";

/* Display string for no timer interval. */
"---" = "---";

/* Units (bytes, kilobytes, etc). */
"B" = "B";
"KB" = "KB";
"MB" = "MB";
"GB" = "GB";
"TB" = "TB";

/* The URL returned a error status code. %u is replaced by the status code, the first %@ is replaced by the human-readable error (automatically localized), the second %@ is replaced by the full URL. */
"The error %u %@ was generated while loading the URL %@." = "L'errore “%u %@” è stato generato caricando l'URL “%@.”";

/* The URL could not be loaded for an unknown reason. %@ is replaced by the full URL. */
"The URL %@ could not be loaded." = "L'URL “%@” non può essere caricato.";

/* Replace file alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"%@ already exists in %@. Do you want to replace it?" = "“%@” esiste già in “%@.” Vuoi sostituirlo?";

/* Replace multiple files alert. %u is replaced with a number greater than 1, %@ is replaced with the destination name. */
"%u pages already exist in %@. Do you want to replace them?" = "%u pagine esistono già “%@.” Vuoi sostituirle?";

/* Informative text for replacement alerts. */
"Replacing a file overwrites its current contents." = "Sostituire un file ne sovrascrive i contenuti attuali.";

/* Extraction window title. */
"Extract" = "Estrai";

/* Extraction single failure alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"The page %@ could not be extracted to %@." = "La pagina “%@” non può essere estratta su “%@.”";

/* Extraction multiple failure alert. %u is replaced with the number of files, %@ is replaced with the destination name. */
"%u pages could not be extracted to %@." = "%u pagine non possono essere estratte su “%@.”";

/* Informative text for extraction failure alerts. */
"Make sure the volume is writable and has enough free space." = "Assicurati che il volume sia scrivibile e abbia abbastanza spazio libero.";

/* Appears in the extraction alert beside each filename that conflicts with an existing file in the destination folder. */
"File already exists." = "Il file esiste già.";

/* Confirmation dialog when the user intentionally deletes a bookmark. %@ is the bookmarked file's name. */
"Are you sure you want to delete the bookmark %@?" = "Are you sure you want to delete the bookmark “%@?”";

/* Confirmation dialog informative text. */
"This operation cannot be undone." = "Questa operazione non può essere annullata.";

/* Bookmarked file could not be found error. %@ is replaced with the missing page's saved filename. */
"The file referenced by the bookmark %@ could not be found." = "Il file cui punta il segnalibro “%@” è introvabile.";

/* Bookmarked file could not be found error informative text. */
"It may have been moved or deleted." = "Potrebbe essere stato spostato o cancellato.";

/* Informative string for secondary mouse button action. */
"Secondary click goes to the previous page." = "Il clic secondario va alla pagina precedente.";
"Secondary click goes right." = "Il clic secondario va a destra.";
"Secondary click goes left." = "Il clic secondario va a sinistra.";

/* Flickr error message. %@ is replaced with the message as returned by Flickr. */
"Flickr returned the error %@." = "Flickr returned the error “%@.”";

/* Flickr user not found error message. %@ is replaced with the user name/NSID. */
"Flickr could not find the user %@. This user may not exist or may have disabled searches in the Flickr privacy settings." = "Flickr could not find the user “%@.” This user may not exist or may have disabled searches in the Flickr privacy settings.";

/* Dialog button labels. */
"OK" = "OK";
"Cancel" = "Annulla";
"Delete Bookmark" = "Elimina preferito";
"Replace" = "Sostituisci";
"Choose" = "Scegli";